# -*- conf -*-

[program:vdi-server-8888]
directory = /opt/veil-vdi/backend
process_name = vdi-server-8888
environment=PYTHONPATH=/opt/veil-vdi/backend
command = /usr/local/bin/pipenv run python app.py --access_to_stdout --logging=debug --port=8888 --debug=True --autoreload=False
stderr_logfile = /var/log/veil-vdi/tornado-stderr.log
stdout_logfile = /var/log/veil-vdi/tornado-stdout.log
autorestart = true
