# -*- conf -*-

[program:vdi-server-8888]
directory = /opt/veil-vdi/backend
process_name = vdi-server-8888
environment=PYTHONPATH=/opt/veil-vdi/backend
command = /usr/local/bin/pipenv run python app.py --access_to_stdout --logging=debug --port=8888 --debug=True --autoreload=False --log_file_prefix=/var/log/veil-vdi/vdi_tornado.log
autorestart = true
